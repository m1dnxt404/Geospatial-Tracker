# OAuth2 client credentials (recommended — from opensky-network.org account settings)
OPENSKY_CLIENT_ID=
OPENSKY_CLIENT_SECRET=
# Legacy basic auth (fallback if OAuth2 credentials are not set)
OPENSKY_USERNAME=
OPENSKY_PASSWORD=
ADSB_API_KEY=
POLLING_INTERVAL_SECONDS=10

# WebSocket URL for the frontend (Vite requires VITE_ prefix)
VITE_WS_URL=ws://localhost:8000/ws/live

# OpenWeatherMap API key — free tier, used for weather tile overlays on the globe
# Register at https://openweathermap.org/api and paste your key below.
VITE_OWM_API_KEY=
